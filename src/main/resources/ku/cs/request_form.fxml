<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.app.controllers.accountOrganizer.RequestFormController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/Request.png" />
         </image>
      </ImageView>
      <TextField fx:id="usernameTextField" layoutX="218.0" layoutY="117.0" prefHeight="35.0" prefWidth="267.0" styleClass="transparent" stylesheets="@../../cssStyle/style.css" />
      <TextField fx:id="passwordTextField" layoutX="218.0" layoutY="159.0" prefHeight="35.0" prefWidth="267.0" styleClass="transparent" stylesheets="@../../cssStyle/style.css" />
      <TextField fx:id="requestTextField" alignment="TOP_LEFT" layoutX="66.0" layoutY="233.0" prefHeight="329.0" prefWidth="667.0" />
      <Button layoutX="643.0" layoutY="174.0" mnemonicParsing="false" onAction="#handleSubmitButton" prefHeight="41.0" prefWidth="80.0" text="Submit">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="14.0" layoutY="9.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="41.0" prefWidth="74.0" styleClass="transparent" stylesheets="@../../cssStyle/style.css" text="Back">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
